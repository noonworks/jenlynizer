{
  "manifest_version": 2,
  
  "name": "Jenlynizer",
  "version": "0.0.1",
  "description": "Jenlynize all.",
  "author": "@noonworks",
  
  "browser_action": {
    "default_title": "Jenlynizer"
  },
  
  "background": { "scripts": ["js/background.js"] },
  
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["js/jenlynize.js"],
      "run_at": "document_start"
    }
  ],
  
  "options_page": "options.html"
}
